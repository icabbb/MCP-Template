{
  "name": "example-mcp",
  "version": "1.0.0",
  "description": "MCP template for Model Context Protocol",
  "type": "module",
  "scripts": {
    "build-tsup": "tsup",
    "build": "tsc && node -e \"require('fs').chmodSync('dist/index.js', '755')\"",
    "process-data": "tsx src/process-data.ts",
    "inspector": "npx -y @modelcontextprotocol/inspector npx -y tsx ./src/index.ts",
    "inspectorSSE": "start cmd /k \"npx tsx ./src/index_sse.ts\" & start cmd /k \"npx -y @modelcontextprotocol/inspector\"",
    "dev": "npx tsx ./src/index_sse.ts"
  },
  "author": "Eduardo Cardoso Martin",
  "license": "ISC",
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.10.1",
    "cheerio": "^1.0.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "pdf-parse": "^1.1.1",
    "pdf2json": "^3.1.5",
    "pdfreader": "^3.0.7",
    "url": "^0.11.4",
    "zod": "3.24.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.14.1",
    "@types/pdf-parse": "^1.1.5",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  }
}
